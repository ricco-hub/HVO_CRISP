        var path = "MonthlyTree_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
        window.location.href="/TREE/Download/"+path;
    }
    function Root2(){
        var now = new Date();
        var month = parseInt(now.getMonth())+1;
        var path = "SolarSmoothed_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
