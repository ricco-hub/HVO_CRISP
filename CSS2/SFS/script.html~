<script>
n =  new Date();
y = n.getFullYear();
m = n.getMonth() + 1;
d = n.getDate();
document.getElementById("date").innerHTML = m + "/" + d + "/" + y;
</script>

<script>
function Root(){
    var now = new Date();
    var month = parseInt(now.getMonth())+1;
    var path = "SSN_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
    window.location.href="/SSN/Download/"+path;
}
</script>

    <script>
