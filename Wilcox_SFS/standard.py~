import requests
import shutil
from DECIMAL import decimal_year
import os
import time

m =int(time.strftime('%m'))
d =int(time.strftime('%d'))
Y =int(time.strftime('%Y'))

with open("/var/www/html/Wilcox_SFS/E.txt", "w") as f:
 f.write('1974.001')
 f.write("\n")
 f.write(str(decimal_year(Y,m,d)))


with open("/var/www/html/Wilcox_SFS/Set.txt", "w") as g:
 g.write('1')
 g.write('2')
g.write('3')
g.write('4')


