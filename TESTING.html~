<div id="drawing" style="width:650px; height:400px"></div>
<script>
function Tree(){
    var now = new Date();
    var month = parseInt(now.getMonth())+1;
    var path = "SSNTree_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
    window.location.href="/TREE/Download/"+path;
}
function Root(){
    var now = new Date();
    var month = parseInt(now.getMonth())+1;
    var path = "SSN_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
    window.location.href="/SSN/Download/"+path;
}
</script>

<button><type="button"><a onclick="Root();">ROOT File</a></button>
<button><type="button"><a onclick="Tree();">ROOT Tree</a></button>

</center>




