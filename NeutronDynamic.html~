<script>
function Tree(){
    var now = new Date();
    var month = parseInt(now.getMonth())+1;
    var path = "NeutronTree_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
    window.location.href="/TREE/Download/"+path;
}
function Root(){
    var now = new Date();
    var month = parseInt(now.getMonth())+1;
    var path = "Neutron_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
    window.location.href="/Neutron/Download/"+path;
}
function Root2(){
    var now = new Date();
    var month = parseInt(now.getMonth())+1;
    var path = "Neutron2_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
    window.location.href="/Neutron2/Download/"+path;
}

function RootFF(){
    var now = new Date();
    var month = parseInt(now.getMonth())+1;
    var path = "ForceField_" + month + "-" + now.getDate() + "-" +now.getFullYear() + ".root";
    window.location.href="/FFModel/Download/"+path;
}

</script>Neutron Monitors Set 1 J(t) Force-Field Model with different E  : <b>JUNG</b>,  <b>KIEL</b>,  <b>OULU</b>,  <b>NEWK</b>
